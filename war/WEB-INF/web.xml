<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Hometask</servlet-name>
		<servlet-class>hometasker.HometaskServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>hometasker.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>hometasker.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Student</servlet-name>
		<servlet-class>hometasker.StudentServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Groups</servlet-name>
		<servlet-class>hometasker.GroupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddGroup</servlet-name>
		<servlet-class>hometasker.AddGroupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Group</servlet-name>
		<servlet-class>hometasker.CurrentGroupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddStudent</servlet-name>
		<servlet-class>hometasker.AddStudentServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateStudentProgress</servlet-name>
		<servlet-class>hometasker.UpdateStudentProgressServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddHometask</servlet-name>
		<servlet-class>hometasker.AddHometaskServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>AddTask</servlet-name>
		<servlet-class>hometasker.AddTaskServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Hometask</servlet-name>
		<url-pattern>/hometask</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Student</servlet-name>
		<url-pattern>/student</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Groups</servlet-name>
		<url-pattern>/groups</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddGroup</servlet-name>
		<url-pattern>/addgroup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Group</servlet-name>
		<url-pattern>/group</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddStudent</servlet-name>
		<url-pattern>/addstudent</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateStudentProgress</servlet-name>
		<url-pattern>/updstudentprogress</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddHometask</servlet-name>
		<url-pattern>/addhometask</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddTask</servlet-name>
		<url-pattern>/addtask</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
